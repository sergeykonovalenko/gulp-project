{
  "name": "gulp-project",
  "version": "2.0.0",
  "description": "Modern Template for Frontend Development on Gulp 5.0.0",
  "type": "module",
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint 'src/**/*.scss' --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,html,scss,css,md}\" --ignore-path .prettierignore",
    "build": "cross-env NODE_ENV=production gulp build",
    "dev": "cross-env NODE_ENV=development gulp",
    "start": "npm run dev"
  },
  "browserslist": [
    "last 2 versions",
    "not IE 11",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sergeykonovalenko/gulp-project.git"
  },
  "author": "Sergey Konovalenko <sergeykonovalenko5550199@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/sergeykonovalenko/gulp-project",
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "autoprefixer": "^10.4.21",
    "browser-sync": "^3.0.3",
    "cross-env": "^7.0.3",
    "gulp": "^5.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-csso": "^4.0.1",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^10.0.0",
    "gulp-posthtml": "^3.0.5",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^6.0.1",
    "gulp-svgstore": "^9.0.0",
    "gulp-terser": "^2.1.0",
    "gulp-webp": "^5.0.0",
    "postcss": "^8.5.3",
    "posthtml-include": "^2.0.1",
    "prettier": "^3.5.3",
    "sass": "^1.85.1"
  },
  "keywords": [
    "gulp",
    "gulp-starter",
    "gulp-boilerplate",
    "frontend",
    "template",
    "starter-kit",
    "boilerplate",
    "sass",
    "scss",
    "babel",
    "webp",
    "svg-sprite",
    "bem",
    "browsersync",
    "postcss",
    "autoprefixer",
    "optimization",
    "minification",
    "development",
    "build-tool",
    "web-development",
    "task-runner"
  ]
}
